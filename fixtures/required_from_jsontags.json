{
  "$ref": "#/definitions/jsonschema.TestUser",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "jsonschema.GrandfatherType": {
      "additionalProperties": false,
      "properties": {
        "family_name": {
          "type": "string"
        }
      },
      "required": ["family_name"],
      "type": "object"
    },
    "jsonschema.TestUser": {
      "additionalProperties": false,
      "properties": {
        "SomeUntaggedBaseProperty": {
          "type": "boolean"
        },
        "TestFlag": {
          "type": "boolean"
        },
        "age": {
          "exclusiveMaximum": true,
          "exclusiveMinimum": true,
          "maximum": 120,
          "minimum": 18,
          "type": "integer"
        },
        "birth_date": {
          "type": "string",
          "format": "date-time"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "feeling": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ]
        },
        "friends": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "grand": {
          "$ref": "#/definitions/jsonschema.GrandfatherType",
          "$schema": "http://json-schema.org/draft-04/schema#"
        },
        "id": {
          "type": "integer"
        },
        "keywords": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "name": {
          "maxLength": 20,
          "minLength": 1,
          "type": "string"
        },
        "network_address": {
          "type": "string",
          "format": "ipv4"
        },
        "nickname": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "photo": {
          "media": {
            "binaryEncoding": "base64"
          },
          "type": "string"
        },
        "secret_float_number": {
          "enum": [
            9.1,
            30.2,
            28.4,
            52.9
          ],
          "type": "number"
        },
        "secret_number": {
          "enum": [
            9,
            30,
            28,
            52
          ],
          "type": "integer"
        },
        "sex": {
          "enum": [
            "male",
            "female",
            "neither",
            "whatever",
            "other",
            "not applicable"
          ],
          "type": "string"
        },
        "some_base_property": {
          "type": "integer"
        },
        "tags": {
          "type": "object"
        },
        "website": {
          "type": "string",
          "format": "uri"
        }
      },
      "required": ["SomeUntaggedBaseProperty", "id", "name", "nickname", "photo"],
      "type": "object"
    }
  }
}