{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"$ref": "#/definitions/CustomMapOuter",
	"definitions": {
		"CustomMapOuter": {
			"type": "object",
			"required": [
				"my_map"
			],
			"additionalProperties": false,
			"properties": {
				"my_map": {
					"items": {
						"required": [
							"key",
							"value"
						],
						"properties": {
							"key": {
								"type": "string"
							},
							"value": {
								"type": "string"
							}
						},
						"type": "object"
					},
					"type": "array"
				}
			}
		}
	}
}
